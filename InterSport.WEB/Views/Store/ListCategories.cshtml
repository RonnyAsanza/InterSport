@using InterSport.Domain.Entities
@model List<Category>
@{
    Layout = null;
}

<div class="card-body">
    <div class="form-check">
        <input class="form-check-input" type="radio" name="inpCategoryName" value="0" id="inpCategoryId" checked="checked" />
        <label class="form-check-label">All</label>
    </div>
    @foreach (var item in Model)
    {
        <div class="form-check">
            <input class="form-check-input" type="radio" name="inpCategoryName" value="@item.CategoryId" id="inpCategoryId" />
            <label class="form-check-label">@item.Description</label>
        </div>
    }
</div>

